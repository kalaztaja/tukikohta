{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/tukikohta-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*"],
  "modes": {
    "window": {
      "title": "tukikohta",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/tukikohta-react/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "tukikohta",
    "resourcesPath": "/tukikohta-react/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/tukikohta-react/public/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0"
  }
}
